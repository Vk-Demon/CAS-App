<?xml version="1.0" encoding="utf-8"?>
<layer-list  xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
       <shape android:shape="oval">
           <gradient
               android:startColor="#87CEFA"
               android:endColor="#EEEEFF"
               android:angle="90"/>
           <stroke android:color="@color/colorGoogleSecondary"
               android:width="1dp"/>
           <size android:width="14dp"
               android:height="14dp"/>
       </shape>
    </item>
    <item android:drawable="@drawable/ic_wb_cloudy_white_14dp"
        android:left="5dp"
        android:right="5dp"
        android:top="5dp"
        android:bottom="5dp"/>
</layer-list>